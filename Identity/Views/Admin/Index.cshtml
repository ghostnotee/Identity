@model IQueryable<Identity.Models.AppUser>

@{
    ViewBag.Title = "Admin Index";
    Layout = "_AdminLayout";
}

<h2>Admin Index</h2>

<table class="table table-bordered table-striped">
    <tr>
        <td>User Id</td>
        <td>User Name</td>
        <td>Email</td>
    </tr>
    @if (!Model.Any())
    {
        <tr>
            <td colspan="3">Hiç bir üye yoktur.</td>
        </tr>
    }
    else
    {
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>
            </tr>
        }
    }
</table>